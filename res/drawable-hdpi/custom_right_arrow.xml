<?xml version="1.0" encoding="utf-8"?>
<selector
	xmlns:android="http://schemas.android.com/apk/res/android">
	<!-- When selected, use orange -->
	<item
		android:drawable="@drawable/right_arrow_orange170"
		android:state_pressed="true" />
	<item
		android:drawable="@drawable/right_arrow_orange170"
		android:state_selected="true" />
	<!-- When not selected, use blue -->
	<item
		android:drawable="@drawable/right_arrow_blue170" />
</selector>